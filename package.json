{
  "name": "BaseTheme",
  "description": "Blank Roadiz theme - This theme is meant to get a fresh start to create a theme.",
  "author": {
    "name": "Rezo Zero",
    "url": "https://www.rezo-zero.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roadiz/BaseTheme"
  },
  "bugs": {
    "url": "https://github.com/roadiz/BaseTheme/issues"
  },
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "better-npm-run dev",
    "watch-svg": "gulp watch-svg --silent",
    "build": "better-npm-run build",
    "build-svg": "gulp build-svg --silent"
  },
  "betterScripts": {
    "dev": {
      "command": "concurrently -r \"npm run watch-svg\" \"webpack-dev-server --watch-poll\"",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "Roadiz-front:*"
      }
    },
    "build": {
      "command": "npm run build-svg && webpack",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "Roadiz-front:*"
      }
    }
  },
