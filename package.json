{
  "name": "BaseTheme",
  "description": "Blank Roadiz theme - This theme is meant to get a fresh start to create a theme.",
  "author": {
    "name": "Rezo Zero",
    "url": "https://www.rezo-zero.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roadiz/BaseTheme"
  },
  "bugs": {
    "url": "https://github.com/roadiz/BaseTheme/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "add-force": "git add --force static/css/* static/img/* static/js/* static/vendors/* Resources/views/svg/sprite.svg.twig Resources/views/partials/css-inject.html.twig Resources/views/partials/js-inject.html.twig  Resources/views/partials/js-inject-legacy.html.twig Resources/views/partials/js-inject-modern.html.twig",
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "dev-livereload": "better-npm-run dev-livereload"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "Roadiz-front:*",
        "REFRESH_ON_CHANGE": false
      }
    },
    "dev-livereload": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "Roadiz-front:*",
        "REFRESH_ON_CHANGE": true
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "Roadiz-front:*"
      }
    }
  },
  "devDependencies": {
    "@babel/cli": "~7.0.0",
    "@babel/core": "~7.0.0",
    "@babel/plugin-proposal-class-properties": "~7.0.0",
    "@babel/plugin-proposal-decorators": "~7.0.0",
    "@babel/plugin-proposal-do-expressions": "~7.0.0",
    "@babel/plugin-proposal-export-default-from": "~7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "~7.0.0",
    "@babel/plugin-proposal-function-bind": "~7.0.0",
    "@babel/plugin-proposal-function-sent": "~7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "~7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.0.0",
    "@babel/plugin-proposal-numeric-separator": "~7.0.0",
    "@babel/plugin-proposal-optional-chaining": "~7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "~7.0.0",
    "@babel/plugin-proposal-throw-expressions": "~7.0.0",
    "@babel/plugin-syntax-dynamic-import": "~7.0.0",
    "@babel/plugin-syntax-import-meta": "~7.0.0",
    "@babel/plugin-transform-runtime": "~7.0.0",
    "@babel/plugin-transform-typescript": "~7.1.0",
    "@babel/preset-env": "~7.0.0",
    "@babel/register": "~7.0.0",
    "@babel/runtime": "~7.1.2",
    "babel-eslint": "~10.0.1",
    "babel-loader": "~8.0.4",
    "better-npm-run": "~0.1.0",
    "bootstrap": "~4.1.3",
    "clean-terminal-webpack-plugin": "~1.1.0",
    "clean-webpack-plugin": "~0.1.19",
    "concurrently": "~4.0.1",
    "copy-webpack-plugin": "~4.4.2",
    "css-loader": "~1.0.0",
    "css-mqpacker": "~7.0.0",
    "cssnano": "~4.1.0",
    "debug": "~3.1.0",
    "del": "~3.0.0",
    "es6-promise": "~4.2.5",
    "eslint": "~5.5.0",
    "eslint-config-standard": "~12.0.0",
    "eslint-loader": "~2.0.0",
    "eslint-plugin-dependencies": "~2.4.0",
    "eslint-plugin-html": "~4.0.5",
    "eslint-plugin-import": "~2.9.0",
