{
  "name": "BaseTheme",
  "description": "Blank Roadiz theme - This theme is meant to get a fresh start to create a theme.",
  "author": {
    "name": "Rezo Zero",
    "url": "https://www.rezo-zero.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roadiz/BaseTheme"
  },
  "bugs": {
    "url": "https://github.com/roadiz/BaseTheme/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "add-force": "git add --force static/css/* static/fonts/* static/img/* static/js/* static/vendors/* Resources/views/svg/sprite.svg.twig Resources/views/partials/css-inject.html.twig Resources/views/partials/js-inject.html.twig",
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "dev-livereload": "better-npm-run dev-livereload"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "Roadiz-front:*",
        "REFRESH_ON_CHANGE": false
      }
    },
    "dev-livereload": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "Roadiz-front:*",
        "REFRESH_ON_CHANGE": true
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "Roadiz-front:*"
      }
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "better-npm-run": "^0.1.0",
    "clean-terminal-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^4.4.2",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.0",
    "debug": "^3.1.0",
    "del": "^3.0.0",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-filter-gradient": "^1.0.0",
    "postcss-fixes": "^2.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-reduce-transforms": "^4.0.0",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.0",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.3.0",
    "svg-sprite-loader": "^3.9.2",
    "svg-transform-loader": "^1.2.1",
    "svgo-loader": "^2.2.0",
    "tapable": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.18.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.0",
    "webpack-disk-plugin": "^0.0.2",
    "webpack-merge": "^4.1.2",
    "webpack-notifier": "^1.5.1",
    "write-file-webpack-plugin": "^4.4.0"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "gsap": "^2.0.2",
    "lazysizes": "^4.1.4",
    "load-google-maps-api": "^1.1.1",
    "starting-blocks": "^5.0.4"
  }
}
