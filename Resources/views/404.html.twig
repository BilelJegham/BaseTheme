{% extends '@BaseTheme/base.html.twig' %}

{% block body_attributes %}class="error error-404" id="error-404"{% endblock %}

{% block content %}
<article id="page-content-404"
         data-title="{{ nodeSource.title }}">
    <h1>Error 404</h1>
    <h2>Requested page cannot be found.</h2>
    {% if errorMessage %}
    <div class="error">{{ errorMessage|markdown }}</div>
    {% endif %}
</article>
{% endblock %}
